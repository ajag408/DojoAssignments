<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Dashboard</title>
  </head>
  <body>
    <h1>Elephants</h1>
    <% for (var i = 0; i < elephants.length; i++) { %>
      <h3><%= elephants[i].name%></h3><br><br>
      <a href = '/elephant/<%= elephants[i].id%>'><button type="button">View Info</button></a>
      <hr>
    <% } %>
    <a href = '/elephants/new'><button type="button">Add elephant</button></a>
  </body>
</html>
